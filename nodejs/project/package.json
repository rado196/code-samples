{
  "name": "@foreach-am/evan-evcahrger-server--service-payments",
  "version": "0.0.1",
  "private": true,
  "author": "forEach LLC",
  "description": "EVAN server Payments microservice.",
  "license": "MIT",
  "scripts": {
    "start": "bash ./scripts/app/serve-development.sh",
    "build": "bash ./scripts/app/build-production.sh",
    "lint": "npm run tool:lint:execute:dev",
    "format": "npm run tool:format:code",
    "start:http": "bash ./scripts/app/serve-dev-service.sh --service-name=http",
    "start:jobs": "bash ./scripts/app/serve-dev-service.sh --service-name=jobs",
    "db:sync": "npm run db:migrate && npm run db:seed",
    "db:migrate": "bash ./scripts/app/run-sequelize.sh db:migrate",
    "db:migrate:undo": "bash ./scripts/app/run-sequelize.sh db:migrate:undo",
    "db:migrate:undo-all": "bash ./scripts/app/run-sequelize.sh db:migrate:undo:all",
    "db:seed": "bash ./scripts/app/run-sequelize.sh db:seed:all",
    "db:seed:undo": "bash ./scripts/app/run-sequelize.sh db:seed:undo",
    "db:seed:undo-all": "bash ./scripts/app/run-sequelize.sh db:seed:undo:all",
    "tool:lint:execute:dev": "bash ./scripts/tools/eslint-execute.sh dev",
    "tool:lint:execute:prod": "bash ./scripts/tools/eslint-execute.sh prod",
    "tool:format:code": "bash ./scripts/tools/format-code.sh"
  },
  "dependencies": {
    "@foreach-am/evan-base-constants": "^1.0.0",
    "@foreach-am/evan-base-library": "^1.0.0",
    "@foreach-am/evan-base-server": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.4",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.15.4",
    "@babel/plugin-transform-modules-umd": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.15.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-preset-minify": "^0.5.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "nodemon": "^2.0.21",
    "prettier": "^2.7.1",
    "prettier-plugin-sh": "^0.12.6",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.7.4"
  }
}
